{
  "name": "tromero",
  "version": "1.2.0",
  "description": "NPM package to conveniently integrate the Tromero service with your Node application. Fully typed.",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/proofoftraining/TS-Tromero.git"
  },
  "bugs": {
    "url": "https://github.com/proofoftraining/TS-Tromero/issues"
  },
  "scripts": {
    "test": "jest",
    "build": "tsup",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack .",
    "lint": "tsc --noEmit",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run lint && npm run test",
    "local-release": "RELEASE_MODE=true npm run ci && changeset version && changeset publish",
    "prepublishOnly": "node prepublish.ts"
  },
  "keywords": [
    "tromero",
    "ai",
    "fine-tuning",
    "finetuning",
    "inference"
  ],
  "author": "Michael Epelboim",
  "license": "MIT",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.4",
    "@changesets/cli": "^2.27.7",
    "@types/jest": "^29.5.12",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "openai": "^4.57.0",
    "uuid": "^10.0.0"
  }
}
